@prefix schema:    <https://schema.org/> .
@prefix region:    <https://mead.example/region/> .
@prefix indicator: <https://mead.example/indicator/> .

# Regions / Places

region:romania
    a schema:Place ;
    schema:identifier "romania" ;
    schema:name "Romania"@en ;
    schema:sameAs
        <https://www.wikidata.org/entity/Q218> ,
        <http://dbpedia.org/resource/Romania> .

region:bucuresti-ilfov
    a schema:Place ;
    schema:identifier "bucuresti-ilfov" ;
    schema:name "Bucure»ôti-Ilfov"@ro ;
    schema:containedInPlace region:romania .

region:nord-est
    a schema:Place ;
    schema:identifier "nord-est" ;
    schema:name "Nord-Est"@ro ;
    schema:containedInPlace region:romania .

# Indicators (definitions of what is being measured)

indicator:obesity-prevalence
    a schema:Dataset ;
    schema:identifier "obesity-prevalence" ;
    schema:name "Obesity prevalence"@en ;
    schema:variableMeasured "Percent of adults with obesity"@en ;
    schema:measurementTechnique "Official statistics / surveys"@en .

indicator:population-density
    a schema:Dataset ;
    schema:identifier "population-density" ;
    schema:name "Population density"@en ;
    schema:variableMeasured "People per square kilometer"@en ;
    schema:measurementTechnique "Official statistics"@en .

indicator:pm25
    a schema:Dataset ;
    schema:identifier "pm25" ;
    schema:name "PM2.5 air pollution"@en ;
    schema:variableMeasured "PM2.5 concentration"@en ;
    schema:measurementTechnique "Air quality monitoring"@en .

indicator:urbanization
    a schema:Dataset ;
    schema:identifier "urbanization" ;
    schema:name "Urbanization rate"@en ;
    schema:variableMeasured "Percent of population living in urban areas"@en ;
    schema:measurementTechnique "Official statistics"@en .
